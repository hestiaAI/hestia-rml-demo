{
  "name": "hestialabs-experiences",
  "description": "We create a new relationship to personal data",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "npm": "7.x",
    "node": "14.x"
  },
  "scripts": {
    "dev": "nuxt --open",
    "test": "jest",
    "start": "nuxt start",
    "build": "npm run build:static",
    "build:static": "nuxt generate",
    "build:server": "nuxt build",
    "lint": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:fix": "npm run lint -- --fix",
    "pre-commit": "lint-staged && node validate-yarrrml",
    "prepare": "husky install"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "^~/(.*)$": "<rootDir>/$1",
      "^vue$": "vue/dist/vue.common.js"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest",
      "^.+\\.vue$": "@vue/vue2-jest"
    }
  },
  "lint-staged": {
    "*.{js,vue}": "npm run lint"
  },
  "dependencies": {
    "@comunica/actor-init-sparql-rdfjs": "^1.22.3",
    "@comunica/logger-pretty": "^1.22.0",
    "@fast-csv/format": "^4.3.5",
    "@fast-csv/parse": "^4.3.6",
    "@nuxtjs/axios": "^5.13.6",
    "@rmlio/yarrrml-parser": "^1.3.2",
    "@types/d3-collection": "^1.0.10",
    "@uppy/core": "^2.1.3",
    "@uppy/dashboard": "^2.0.2",
    "@uppy/drop-target": "^1.0.1",
    "core-js": "^3.19.3",
    "crossfilter2": "^1.5.4",
    "d3": "^7.1.1",
    "d3-axis": "^3.0.0",
    "d3-collection": "^1.0.7",
    "d3-sankey": "^0.12.3",
    "d3-shape": "^3.0.1",
    "dc": "^4.2.7",
    "file-saver": "^2.0.5",
    "jsonpath-plus": "^6.0.1",
    "jszip": "^3.7.1",
    "libsodium-wrappers": "^0.7.9",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "n3": "^1.12.1",
    "nuxt": "^2.15.8",
    "regression": "^2.0.1",
    "rocketrml": "^1.11.2",
    "sha1": "^1.1.1",
    "sparqlalgebrajs": "^4.0.1",
    "sql.js": "^1.6.2",
    "unzipit": "^1.3.5",
    "vega": "^5.20.2",
    "vega-embed": "^6.18.2",
    "vega-lite": "^5.1.1",
    "vue-social-sharing": "^3.0.8",
    "vuetify": "^2.6.1",
    "vuex-persistedstate": "^4.1.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@babel/preset-env": "^7.16.4",
    "@mdi/js": "^6.5.95",
    "@nuxtjs/eslint-config": "^7.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/vuetify": "^1.12.1",
    "@vue/preload-webpack-plugin": "^2.0.0",
    "@vue/test-utils": "^1.3.0",
    "@vue/vue2-jest": "^27.0.0-alpha.3",
    "babel-core": "^7.0.0-bridge.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "file-loader": "^6.2.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "raw-loader": "^4.0.2",
    "regenerator-runtime": "^0.13.9",
    "vue2-ace-editor": "^0.0.15",
    "worker-loader": "^3.0.8"
  }
}
