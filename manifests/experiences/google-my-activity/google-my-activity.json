{
  "title": "Google My Activity",
  "subtitle": "My Activity",
  "icon": "google.png",
  "dataPortal": "https://takeout.google.com/settings/takeout",
  "ext": "zip",
  "data": [],
  "timedObservationsViewer": {
    "fileMatchers": [
      {
        "regex": "Takeout/(.+?)/(.+?)/(.+?)\\.json",
        "entryPath": "$[*]",
        "paths": {
          "date": "$.time",
          "eventSource": "$.products[0]",
          "eventType": "$.title",
          "eventValue": "$.title"
        }
      }
    ],
    "fields": ["date", "eventSource", "eventType", "eventValue"],
    "parser": "groupSimilarEventValues"
  },
  "defaultView": [
    {
      "customPipeline": "timedObservationViewer",
      "visualizations": ["TimedObservationsViewer.vue"],
      "showTable": false,
      "title": "Overview of what google knows about you",
      "text": ""
    }
  ]
}
