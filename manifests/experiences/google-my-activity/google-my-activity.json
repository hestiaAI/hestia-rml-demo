{
  "title": "Google My Activity",
  "subtitle": "My Activity",
  "icon": "google.png",
  "dataPortal": "https://takeout.google.com/settings/takeout",
  "ext": "zip",
  "data": [],
  "timedObservationsViewer": {
    "fileMatchers": [
      {
        "regex": "Takeout/(?:.+?)/(.+?)/(?:.+?)\\.json",
        "entryPath": "$[*]",
        "fields": {
          "date": {
            "source": "entry",
            "path": "$.time"
          },
          "eventValue": {
            "source": "entry",
            "path": "$.title"
          },
          "eventSource": {
            "source": "regex",
            "path": "1"
          }
        }
      }
    ],
    "parser": "identifyTypeFromManifestList",
    "eventTypes": {
      "en": {
        "Drive": ["bulk download"],
        "Google Pay": ["contactless payment that failed", "contactless payment", "Used Google Pay"],
        "Takeout": ["Downloaded a Takeout archive", "Initiated a Takeout"],
        "YouTube": ["Liked", "Disliked", "Subscribed to"]
      },
      "fr": {
        "Drive": ["téléchargement groupé"],
        "Google Pay": ["paiement sans contact qui a échoué", "paiement sans contact", "Application utilisée"],
        "Takeout": ["lancé une opération", "téléchargé une archive"],
        "YouTube": ["A aimé", "N'a pas aimé", "Vous vous êtes abonné à"]
      },
      "fi": {

      }
    }
  },
  "defaultView": [
    {
      "customPipeline": "timedObservationViewer",
      "visualizations": ["TimedObservationsViewer.vue"],
      "showTable": false,
      "title": "Overview of what google knows about you",
      "text": ""
    }
  ]
}
