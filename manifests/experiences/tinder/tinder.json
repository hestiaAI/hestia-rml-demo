{
  "title": "Tinder",
  "icon": "tinder.png",
  "dataPortal": "https://account.gotinder.com/data",
  "data": ["tinder-synthetic.json"],
  "hideSummary": true,
  "hideFileExplorer": true,
  "files": {
    "tinder": "**/*.json"
  },
  "preprocessors": {
    "**/*.json": "tinder"
  },
  "collaborator": {
    "title": "The Dating Privacy Collective",
    "icon": "dating-privacy-collective.svg",
    "url": "https://dating-privacy.hestialabs.org/"
  },
  "defaultView": [
    {
      "key": "likes-dislikes-vue",
      "sql": "likes-dislikes",
      "files": ["tinder"],
      "postprocessor": "likesDislikesData",
      "visualization": "ChartViewTimeSeries.vue",
      "vizProps": {
        "title": "Likes vs Passes over time",
        "legendOffset": 350,
        "dateAccessor": { "text": "Date", "value": "date" },
        "seriesAccessor": { "text": "Action", "value": "action" },
        "valueAccessor": "value"
      },
      "showTable": false,
      "title": "Likes and passes",
      "text": "See the number of likes and passes that you made over time."
    },
    {
      "key": "app-opens-vue",
      "sql": "app-opens",
      "files": ["tinder"],
      "postprocessor": "appOpensData",
      "visualization": "ChartViewTimeSeries.vue",
      "vizProps": {
        "title": "Application usage over time",
        "legendOffset": 360,
        "dateAccessor": { "text": "Date", "value": "date" },
        "seriesAccessor": { "text": "Action", "value": "action" },
        "valueAccessor": "value"
      },
      "showTable": false,
      "title": "App openings",
      "text": "See the number of times that you opened the app (per day or per month)."
    },
    {
      "key": "monthly-history-vue",
      "sql": "likes-dislikes-messages",
      "files": ["tinder"],
      "postprocessor": "likesDislikesMessagesData",
      "visualization": "ChartViewTimeSeries.vue",
      "vizProps": {
        "title": "Activity history",
        "legendOffset": 200,
        "dateAccessor": { "text": "Date", "value": "date" },
        "seriesAccessor": { "text": "Action", "value": "action" },
        "valueAccessor": "value"
      },
      "showTable": false,
      "title": "Likes, passes, messages",
      "text": "Compare the number of likes, passes, messages sent and messages received."
    },
    {
      "key": "donut",
      "sql": "all",
      "files": ["tinder"],
      "visualization": "donut-multiple-comparisons",
      "showTable": false,
      "title": "Pairs",
      "text": "Compare different pairs of values in a donut chart."
    },
    {
      "key": "likes-dislikes-open",
      "sql": "likes-dislikes-open",
      "files": ["tinder"],
      "visualization": "scatter-likes-passes-correlation",
      "showTable": false,
      "title": "Likes / passes correlation",
      "text": "Observe the possible correlation between the number of likes/passes and the number of times you open the app."
    },
    {
      "key": "all",
      "sql": "all",
      "files": ["tinder"],
      "showTable": true,
      "title": "All data",
      "text": "If you want to inspect precisely the values for each day, all the data can be summarized in a table."
    }
  ]
}
