{
  "title": "Facebook",
  "icon": "facebk.png",
  "ext": "zip",
  "preprocessor": "facebook",
  "files": [
    "apps_and_websites_off_of_facebook/your_off-facebook_activity.json", 
    "your_topics/your_topics.json", 
    "ads_information/advertisers_you've_interacted_with.json", 
    "ads_information/advertisers_who_uploaded_a_contact_list_with_your_information.json"
  ],
  "allowMissingFiles": true,
  "data": [],
  "csvProcessorNames": {"main": {"off-facebook-activity-type-count": "sunburstTargetingAdvertiser"}},
  "visualizations": {"main": {"off-facebook-activity-count": "top-k-advertisers", "off-facebook-activity-type-count": "sunburst-off-facebook-activity"}},
  "defaultView": [
    {
      "query": "off-facebook-activity-count",
      "visualizations": ["top-k-advertisers"],
      "showTable": false,
      "text": "See how many times advertisers have informed Facebook of your activity outside of Facebook."
    },
    {
      "query": "off-facebook-activity-type-count",
      "visualizations": ["sunburst-off-facebook-activity"],
      "showTable": true,
      "text": "See which kind of information advertisers have informed Facebook of your activity outside of Facebook."
    },
    {
      "query": "ad-interactions",
      "showTable": true,
      "text": "See the list of ads with which you have interacted."
    },
    {
      "query": "your-topics",
      "showTable": true,
      "text": "Get a list of topics that Facebook thinks you're interested in."
    },
    {
      "query": "advertisers-contact-list",
      "showTable": true,
      "text": "Get a list of advertisers that have uploaded a contact list containing you."
    }
  ]
}
