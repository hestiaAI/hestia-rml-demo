  "data": [],
  "hideSummary": true,
  "hideFileExplorer": true,
  "files": {
    "userdata": "**/user_data.json"
  },
  "collaborator": {
    "title": "The Eyeballs Collective",
    "icon": "the-eyeballs.svg",
