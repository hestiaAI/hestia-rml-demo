  },
  "defaultView": [
    {
      "key": "comments",
      "sql": "comments",
      "files": ["comments"],
      "showTable": true,
      "title": "Comments",
      "text": "Comments recieved"
    }
  ],
  "databaseConfig": {
    "tables": [
      {
        "name": "TiktokComment",
        "columns": [
          ["date", "TEXT"],
          ["comment", "TEXT"]
        ]
